{
    "name": "dhtml/flarum-language-translator",
    "description": "Flarum Language Translator",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "GPL-1.0-or-later",
    "require": {
        "flarum/core": "^1.2.0",
		 "google/cloud-translate": "^1.17",
        "guzzlehttp/guzzle": "^7.0.1",
    },
    "authors": [
        {
            "name": "Anthony Ogundipe",
            "email": "diltony@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dhtml\\FlarumLanguageTranslator\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Flarum Language Translator",
            "category": "feature",
            "icon": {
                "name": "far fa-eye",
                "backgroundColor": "#008000",
                "color": "#fff"
            }
        },
        "flarum-cli": {
            "modules": {
                "admin": true,
                "forum": true,
                "js": true,
                "jsCommon": true,
                "css": true,
                "locale": true,
                "gitConf": true,
                "githubActions": true,
                "prettier": true,
                "typescript": true,
                "bundlewatch": false,
                "backendTesting": true,
                "editorConfig": true,
                "styleci": true
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
